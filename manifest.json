{
  "name": "Tailored Document Search",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "A chromium extension to aid searching documents.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "permissions": [
    "sidePanel", "tabs"
  ]
}